{% extends '::base.html.twig' %}
{% block title %}Patron Feedback Type{% endblock %}
{% block body %}  
            
    {# Page Heading #}
    {% set headingvars = {'headingcontent' : 'Feedback Type: ' ~ entity.name } %}
    {% include 'snippets/pageHeading.html.twig' with headingvars %}
    
    {# Back button, edit and print buttons #}
    {% set toolbarvars = {'basepath' : path('feedbackcategory'), 'editpath' : path('feedbackcategory_edit', { 'id': entity.id }), 'printpath' : path('feedbackcategory_print', { 'id': entity.id }), 'editrole' : 'ROLE_FEEDBACK_EDIT'} %}
    {% include 'snippets/showToolbar.html.twig' with toolbarvars %}
    
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-6">
          <table class="table table-responsive table-striped">
              <caption>Type Information</caption>
              <tbody>
                  <tr>
                      <td><strong>Type</strong></td>
                      <td>{{entity.name}}</td>
                  </tr>
              </tbody>
            </table>
      </div>
    </div>
{% endblock %}
