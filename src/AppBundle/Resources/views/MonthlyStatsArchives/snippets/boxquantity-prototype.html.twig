<div class="boxquantity-identifier" id="appbundle_monthlystatsarchives_requestedCollections___collectionname___boxQuantity___boxname__">
    <div class="form-group">
        <label class="control-label required" for="appbundle_monthlystatsarchives_requestedCollections___collectionname___boxQuantity___boxname___box">Box Number</label>
        <select id="appbundle_monthlystatsarchives_requestedCollections___collectionname___boxQuantity___boxname___box" name="appbundle_monthlystatsarchives[requestedCollections][__collectionname__][boxQuantity][__boxname__][box]" required="required" class="form-control">
            <option value="" selected="selected">{{ form.children.box.vars.placeholder }}</option>
            {% for option in form.children.box.vars.choices %}
                <option value="{{ option.data.id }}">{{ option.data.boxNumber }}</option>
            {% endfor %}
        </select>
    </div>
    <div class="form-group">
        <label class="control-label required" for="appbundle_monthlystatsarchives_requestedCollections___collectionname___boxQuantity___boxname___quantity">Quantity</label>
        <input type="number" id="appbundle_monthlystatsarchives_requestedCollections___collectionname___boxQuantity___boxname___quantity" name="appbundle_monthlystatsarchives[requestedCollections][__collectionname__][boxQuantity][__boxname__][quantity]" required="required" class="form-control" />
    </div>
</div>