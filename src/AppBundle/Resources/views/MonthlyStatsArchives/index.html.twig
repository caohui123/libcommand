{% extends '::base.html.twig' %}

{% block body -%}
    <h1>MonthlyStatsArchives list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Month</th>
                <th>Researchminutes5</th>
                <th>Researchminutes10</th>
                <th>Researchminutes15</th>
                <th>Researchminutes20</th>
                <th>Researchminutes30</th>
                <th>Researchminutes45</th>
                <th>Researchminutes60</th>
                <th>Instructionalminutes5</th>
                <th>Instructionalminutes10</th>
                <th>Instructionalminutes15</th>
                <th>Instructionalminutes20</th>
                <th>Instructionalminutes30</th>
                <th>Instructionalminutes45</th>
                <th>Instructionalminutes60</th>
                <th>Researchersfaculty</th>
                <th>Researchersstaff</th>
                <th>Researchersundergrad</th>
