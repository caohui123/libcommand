                <th>Collectionsstoredtotalcollection</th>
                <th>Holdingsaddedbooks</th>
                <th>Holdingsaddedfacultypublications</th>
                <th>Accessionslinearfeet</th>
                <th>Accessionstotalcollections</th>
                <th>Created</th>
                <th>Updated</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('monthly_archives_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{% if entity.month %}{{ entity.month|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.researchMinutes5 }}</td>
                <td>{{ entity.researchMinutes10 }}</td>
                <td>{{ entity.researchMinutes15 }}</td>
                <td>{{ entity.researchMinutes20 }}</td>
                <td>{{ entity.researchMinutes30 }}</td>
                <td>{{ entity.researchMinutes45 }}</td>
                <td>{{ entity.researchMinutes60 }}</td>
                <td>{{ entity.instructionalMinutes5 }}</td>
                <td>{{ entity.instructionalMinutes10 }}</td>
                <td>{{ entity.instructionalMinutes15 }}</td>
                <td>{{ entity.instructionalMinutes20 }}</td>
                <td>{{ entity.instructionalMinutes30 }}</td>
                <td>{{ entity.instructionalMinutes45 }}</td>
                <td>{{ entity.instructionalMinutes60 }}</td>
                <td>{{ entity.researchersFaculty }}</td>
                <td>{{ entity.researchersStaff }}</td>
                <td>{{ entity.researchersUndergrad }}</td>
                <td>{{ entity.researchersGrad }}</td>
                <td>{{ entity.researchersCommunity }}</td>
                <td>{{ entity.researchersOther }}</td>
                <td>{{ entity.directionalEmailRef }}</td>
                <td>{{ entity.directionalPhoneRef }}</td>
                <td>{{ entity.researchRequestsCollectionEmailRef }}</td>
                <td>{{ entity.researchRequestsCollectionPhoneRef }}</td>
                <td>{{ entity.researchRequestsEmailRef }}</td>
                <td>{{ entity.researchRequestsPhoneRef }}</td>
                <td>{{ entity.donationsEmailRef }}</td>
                <td>{{ entity.donationsPhoneRef }}</td>
                <td>{{ entity.loansEmailRef }}</td>
                <td>{{ entity.loansPhoneRef }}</td>
                <td>{{ entity.collectionsProcessedLinearFeet }}</td>
                <td>{{ entity.collectionsProcessedCallNumber }}</td>
                <td>{{ entity.collectionsProcessedTotalCollection }}</td>
                <td>{{ entity.collectionsStoredLinearFeet }}</td>
                <td>{{ entity.collectionsStoredCallNumber }}</td>
                <td>{{ entity.collectionsStoredTotalCollection }}</td>
                <td>{{ entity.holdingsAddedBooks }}</td>
                <td>{{ entity.holdingsAddedFacultyPublications }}</td>
                <td>{{ entity.accessionsLinearFeet }}</td>
                <td>{{ entity.accessionsTotalCollections }}</td>
                <td>{% if entity.created %}{{ entity.created|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.updated %}{{ entity.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('monthly_archives_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('monthly_archives_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
