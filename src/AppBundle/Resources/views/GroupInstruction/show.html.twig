{% extends '::base.html.twig' %}

{% block body -%}
    <h1>GroupInstruction</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Instructor</th>
                <td>{{ entity.instructor }}</td>
            </tr>
            <tr>
                <th>Attendance</th>
                <td>{{ entity.attendance }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ entity.level }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('groupinstruction') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('groupinstruction_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
