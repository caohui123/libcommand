<thead>
    <tr>
        <th>{{category}}</th>
    {% if type == 'calendar' %}
        <th>Jan '{{year|slice(2,2)}}</th>
        <th>Feb '{{year|slice(2,2)}}</th>
        <th>Mar '{{year|slice(2,2)}}</th>
        <th>Apr '{{year|slice(2,2)}}</th>
        <th>May '{{year|slice(2,2)}}</th>
        <th>Jun '{{year|slice(2,2)}}</th>
        <th>Jul '{{year|slice(2,2)}}</th>
        <th>Aug '{{year|slice(2,2)}}</th>
        <th>Sep '{{year|slice(2,2)}}</th>
        <th>Oct '{{year|slice(2,2)}}</th>
        <th>Nov '{{year|slice(2,2)}}</th>
        <th>Dec '{{year|slice(2,2)}}</th>
    {% elseif type == 'fiscal' %}
        <th>Jul '{{year|slice(2,2)}}</th>   
        <th>Aug '{{year|slice(2,2)}}</th>  
        <th>Sep '{{year|slice(2,2)}}</th>  
        <th>Oct '{{year|slice(2,2)}}</th>  
        <th>Nov '{{year|slice(2,2)}}</th>  
        <th>Dec '{{year|slice(2,2)}}</th>  
        <th>Jan '{{year|slice(7,2)}}</th>  
        <th>Feb '{{year|slice(7,2)}}</th>  
        <th>Mar '{{year|slice(7,2)}}</th>  
        <th>Apr '{{year|slice(7,2)}}</th>  
        <th>May '{{year|slice(7,2)}}</th>  
        <th>Jun '{{year|slice(7,2)}}</th>  
    {% endif %}
        <th>TOTAL</th>
    <tr>
</thead>